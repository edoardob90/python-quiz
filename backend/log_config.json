{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "unified": {
      "format": "%(asctime)s | %(levelname)-5s | %(name)-5s | %(message)s",
      "datefmt": "%H:%M:%S"
    },
    "access": {
      "()": "uvicorn.logging.AccessFormatter",
      "fmt": "%(asctime)s | %(levelname)-5s | %(name)-5s | %(client_addr)s - \"%(request_line)s\" %(status_code)s",
      "datefmt": "%H:%M:%S"
    }
  },
  "handlers": {
    "default": {
      "formatter": "unified",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stderr"
    },
    "access": {
      "formatter": "access",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "level": "INFO"
    },
    "uvicorn.access": {
      "handlers": ["access"],
      "level": "INFO",
      "propagate": false
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["default"]
  }
}
